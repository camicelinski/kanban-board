{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "plugins": [
      "react"
  ],
  "rules": {
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1,
          "ignoredNodes": [
            "ConditionalExpression"
          ]
        }
      ],
      "func-names": [
        "error",
        "as-needed"
      ]
  }
}